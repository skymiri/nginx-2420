server {
    listen 80;
    server_name 2420_Yoon;
    root /srv/2420-files;
    location / {
	    proxy_pass http://143.198.229.89/;
	    proxy_set_header Host $Host;
	    proxy_set_header X-Real-IP
		    $remote_addr;
	    proxy_set_header X-Forwarded-For
		    $proxy_add_x_forwarded_for;
	    proxy_set_header X-Forwarded-Proto
		    $scheme;
    }

}
